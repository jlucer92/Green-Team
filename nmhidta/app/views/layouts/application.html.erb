<!DOCTYPE html>
<html>
<head>
  <title>New Mexico High Intensity Drug Trafficking Agency</title>
  <%= stylesheet_link_tag    "nmhidta", :media => "all" %>
  <%= stylesheet_link_tag "screen" %>
  <%= stylesheet_link_tag "css_page" %>
  <%= stylesheet_link_tag "s3slider" %>
  <%= stylesheet_link_tag "templatemo_style" %>
  <%= javascript_include_tag "application" %>
  <%= javascript_include_tag "jquery" %>
  <%= javascript_include_tag "jquery.min" %>
  <%= javascript_include_tag "piroBox.1_2" %>
  <%= javascript_include_tag "piroBox.1_2_min" %>
  <%= javascript_include_tag "s3slider" %> 
  <%= csrf_meta_tags %>
</head>
<body>


<div id="user_nav">
<% if current_user %>
Logged in as <%= current_user.email %>.
<%= link_to "Log out", log_out_path %>
<% else %>
<%= link_to "Sign up", sign_up_path %> or
<%= link_to "log in", log_in_path %>
<% end %>
</div>


<% flash.each do |name, msg| %>
<%= content_tag :div, msg, :id => "flash_#{name}" %>

<% end %>





<%= yield %>

</body>
</html>
